<Window x:Class="Learning_WPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Learning_WPF"
        xmlns:fa5="http://schemas.fontawesome.com/icons/"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Window.Resources>
       
    </Window.Resources>
    <DockPanel x:Name="doc" Background="Bisque">
        <DockPanel VerticalAlignment="Top" Height="40" Background="#FFDD9191">
            <Button x:Name="page1" fa5:Awesome.Content="Regular_ArrowAltCircleLeft" 
                    HorizontalAlignment="Left" Width="60" Background="Bisque"
                    FontSize="30" PreviewMouseDown="page1_click"/>
            <Button x:Name="page2" fa5:Awesome.Content="Regular_ArrowAltCircleRight" 
                    HorizontalAlignment="Left" Width="60" Background="Bisque"
                    FontSize="30" PreviewMouseDown="page2_click"/>
        </DockPanel>
        <Frame x:Name="MainFrame" Margin="-120,0,0,0" NavigationUIVisibility="Hidden" 
               VerticalAlignment="Bottom" Height="380" HorizontalAlignment="Stretch"/>
    </DockPanel>
   
</Window>
